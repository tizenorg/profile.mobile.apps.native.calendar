#define EDIT_REPEAT_UNTIL_SWALLOW_TIMES_WIDTH 140

group { name: "elm/genlist/item/CalDialogEditRepeatUntilTimesItem/default";
   data.item: "treesize" GENLIST_TREESIZE_DEFAULT;
   data.item: "texts" "elm.text";
   data.item: "contents" "elm.icon.right elm.swallow.times";
   data.item: "banded_bg_area" "elm.swallow.bg";
   data.item: "banded_bg_color" "255 255 255 255";

   parts {
      part { name: "elm.swallow.bg";
            type: SWALLOW;
                scale: 1;
                description { state: "default" 0.0;
            }
      }
      PART(SPACER, "elm.padding.left",
         DSC_FROM_L("default" 0.0,
            min: LIST_PADDING_DEFAULT_SIZE 0;
            max: LIST_PADDING_DEFAULT_SIZE -1;
         )
      )
      PART(SPACER, "elm.padding.right",
         DSC_FROM_R("default" 0.0,
            min: LIST_PADDING_DEFAULT_SIZE 0;
            max: LIST_PADDING_DEFAULT_SIZE -1;
         )
      )
      PART(SPACER, "elm.padding.top",
         DSC_FROM_T("default" 0.0,
            min: 0 LIST_PADDING_DEFAULT_SIZE;
            max: -1 LIST_PADDING_DEFAULT_SIZE;
         )
      )
      PART(SPACER, "elm.padding.bottom",
         DSC_FROM_B("default" 0.0,
            min: 0 LIST_PADDING_DEFAULT_SIZE;
            max: -1 LIST_PADDING_DEFAULT_SIZE;
         )
      )
      PART(SPACER, "elm.padding.times.left",
         DSC_L("default" 0.0,
            "elm.swallow.times",
            min: LIST_PADDING_DEFAULT_SIZE 0;
            max: LIST_PADDING_DEFAULT_SIZE -1;
         )
      )
      PART(SWALLOW, "elm.swallow.times",
         DSC_LTB("default" 0.0,
            "elm.padding.left", "elm.padding.top", "elm.padding.bottom",
            min: EDIT_REPEAT_UNTIL_SWALLOW_TIMES_WIDTH 0;
            max: EDIT_REPEAT_UNTIL_SWALLOW_TIMES_WIDTH -1;
            fixed: 0 1;
            rel2.relative: 1 0;
            rel2.to_x: "elm.padding.times.left";
            align: 0.0 0.5;
         )
      )
      PART(SPACER, "elm.padding.times.right",
         DSC_L("default" 0.0,
            "elm.swallow.times",
            min: LIST_PADDING_DEFAULT_SIZE 0;
            max: LIST_PADDING_DEFAULT_SIZE -1;
         )
      )
      PART(TEXTBLOCK, "elm.text",
         DSC_LRTB("default" 0.0,
            "elm.padding.times.left", "elm.icon.right", "elm.padding.top", "elm.padding.bottom",
            min: 0 56;
            text.min: 0 1;
            text.style: "list_text_main";
            align: 0.0 0.5;
         )
      )
      PART(SWALLOW, "elm.icon.right",
         DSC_R("default" 0.0, "elm.padding.right", )
      )
   }
   programs {
        LIST_PROGRAM_SOUND
   }
}

group { name: "elm/genlist/item/CalDialogEditRepeatUntilTimesItemRTL/default";
   data.item: "treesize" GENLIST_TREESIZE_DEFAULT;
   data.item: "texts" "elm.text.right";
   data.item: "contents" "elm.icon.right elm.swallow.times";
