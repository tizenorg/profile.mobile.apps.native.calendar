   data.item: "banded_bg_color" "255 255 255 255";

   parts {
      part { name: "elm.swallow.bg";
            type: SWALLOW;
                scale: 1;
                description { state: "default" 0.0;
            }
      }
      //LIST_PART_BG
      PART(SPACER, "elm.padding.left",
         DSC_FROM_L("default" 0.0,
            min: 0 0;
            max: 0 -1;
         )
      )
      PART(SPACER, "elm.padding.right",
         DSC_FROM_R("default" 0.0,
            min: LIST_PADDING_DEFAULT_SIZE 0;
            max: LIST_PADDING_DEFAULT_SIZE -1;
         )
      )
      PART(SPACER, "elm.padding.top",
         DSC_FROM_T("default" 0.0,
            min: 0 LIST_PADDING_DEFAULT_SIZE;
            max: -1 LIST_PADDING_DEFAULT_SIZE;
         )
      )
      PART(SPACER, "elm.padding.bottom",
         DSC_FROM_B("default" 0.0,
            min: 0 LIST_PADDING_DEFAULT_SIZE;
            max: -1 LIST_PADDING_DEFAULT_SIZE;
         )
      )
      PART(SWALLOW, "elm.icon.right",
         DSC_R("default" 0.0, "elm.padding.right", )
      )
      PART(SPACER, "elm.padding.times.right",
         DSC_R("default" 0.0,
            "elm.icon.right",
            min: 15 0;
            max: 15 -1;
         )
      )
      PART(SWALLOW, "elm.swallow.times",
         DSC_RTB("default" 0.0,
            "elm.padding.times.right", "elm.padding.top", "elm.padding.bottom",
            min: EDIT_REPEAT_UNTIL_SWALLOW_TIMES_WIDTH 60;
            fixed: 1 1;
         )
      )
      PART(SPACER, "elm.padding.times.left",
         DSC_R("default" 0.0,
            "elm.swallow.times",
            min: LIST_PADDING_DEFAULT_SIZE 0;
            max: LIST_PADDING_DEFAULT_SIZE -1;
         )
      )
      PART(TEXTBLOCK, "elm.text.right",
         DSC_LRTB("default" 0.0,
            "elm.padding.left","elm.padding.times.left", "elm.padding.top", "elm.padding.bottom",
            min: 0 56;
            text.min: 0 1;
            text.style: "list_text_main_repeat_item";
            align: 1.0 0.5;
         )
      )
   }
   programs {
        LIST_PROGRAM_SOUND
   }
}
